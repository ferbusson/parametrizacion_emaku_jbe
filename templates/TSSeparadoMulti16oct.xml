<emaku_template type="POSTSCRIPT" requestNumeration="false" copies="1" silent="false" printer="EPSON_TM-T20">
   <settings width="220" height="10000" />

   <metadata>
      <font row="0" col="0" name="Sans" size="10" />
      <field row="150" center="100" type="STRING" importValue="true">nombre_suc</field>
      <font row="0" col="0" name="Sans" size="7" />
      <field row="160" center="130" type="STRING" importValue="true">nitalterno</field>
      
      <font row="0" col="0" name="Sans" size="8" />
      <field row="190" col="15" type="STRING">SEPARADO MERCANCIA (Copia)</field>
      <field row="210" col="15" type="STRING">Numero:</field>

      <field row="220" col="15" type="STRING">Fecha:</field>
      <field row="230" col="15" type="STRING">Cajero:</field>
      <field row="240" col="15" type="STRING">Hora:</field>
      <line row="250" col="2" row2="250" col2="200" />
      <field row="260" col="13" type="STRING">Código</field>
      <field row="270" col="60" type="STRING">Descripcion</field>
      <field row="280" col="10" type="STRING">Cant</field>
      <field row="280" col="60" type="STRING">V. Unit</field>
      <field row="280" col="130" type="STRING">Total</field>
      <field row="280" col="170" type="STRING">T. USD</field>
      <line row="290" col="2" row2="290" col2="200" />
   </metadata>


 <!-- 0 logo sucursal -->
      <package>
      <dbimage row="50" width = "150" height= "80"  col="15" type="STRING" />
      </package>
  <!-- 1 -->
<!--  <package>
          <generateBarCodeImage row="110" col="20" width="10" type="NUMERIC" mask="0000000000" />
  </package>-->

	<!-- 2 Numero fac-->
	 <package>
        <field row="210" col="100" type="STRING" />
    </package>
    <!-- 3 Codigo num fac-->
     <package>
       <field row="210" col="53" type="STRING" />
     </package>

	<!-- 4 Hora-->
	<package>
          <field row="240" col="53" type="STRING" />
  </package>
   <!-- 5 Fecha Factura -->
   <package>
      <field row="220" col="53" type="DATE" mask="EEEEEE dd MMMM yyyy" />
   </package>
   <!-- 6 Cajero -->
   <package>
      <field row="230" col="53" type="STRING" />
   </package>
   <!-- 7 Tabla principal productos -->
   <package>
      <subpackage rowInit="300" rowAcum="10">
         <field col="15" type="STRING" incrementRow="true" />
         <field col="15" incrementRow="true" type="STRING" />
         <field col="15" width="3" type="NUMERIC" mask="###" />
         <field col="80" width="13" type="NUMERIC" mask="#,###,###.##" />
         <field col="140" width="13" type="NUMERIC" mask="#,###,###.##" />
         <field col="187" width="13" type="NUMERIC" mask="###.##" />
      </subpackage>
   </package>
   <!-- 8 Linea -->
   <package>
      <line row="last" rowAcum="20" col="2" row2="last" col2="200" />
   </package>  
    <!-- 9 Total -->
   <package>
      <field textLine="Total ......" rowAcum="20" textCol="12" fontName="Dialog" fontSize="14" fontStyle="1" row="last" col="200" width="13" type="NUMERIC" mask="#,###,###.##" />
   </package>  
   <!-- 10 Valor Separado -->
   <package>
      <field textLine="Valor Separado" rowAcum="20" textCol="12" fontName="Dialog" fontSize="14" fontStyle="1" row="last" col="200" width="13" type="NUMERIC" mask="#,###,###.##" />
   </package>
   <!-- 11 Saldo Separado-->
   <package>
      <field textLine="Saldo Separado" rowAcum="20" textCol="12" fontName="Dialog" fontSize="14" fontStyle="1" row="last" col="200" width="13" type="NUMERIC" mask="#,###,###.##" />
   </package>
   <!-- 12 Efectivo -->
   <package>
      <field textLine="Efectivo ......" rowAcum="20" textCol="12" validZero="true" fontName="Dialog" fontSize="14" fontStyle="1" row="last" col="200" width="13" type="NUMERIC" mask="#,###,###.##" />
   </package>  
   <!-- 13 Total pago tarjetas -->
   <package>
      <field textLine="Tarjetas ......" rowAcum="20" textCol="12"  validZero="true" fontName="Dialog" fontSize="14" fontStyle="1" row="last" col="200" width="13" type="NUMERIC" mask="#,###,###.##" />
   </package>  

   <!-- 14 Sodexo -->
   <package>
      <field textLine="Sodexo/BigPass" rowAcum="20" textCol="12" validZero="true" fontName="Dialog" fontSize="14" fontStyle="1" row="last" col="200" width="13" type="NUMERIC" mask="#,###,###.##" />
   </package>  
   <!-- 15 Cambio -->
   <package>
      <field textLine="Cambio ......" rowAcum="15" textCol="12" fontName="Dialog" fontSize="14" fontStyle="1" row="last" col="200" width="13" type="NUMERIC" mask="#,###,###.##" />
   </package>  
   <!-- 16 Linea -->
   <package>
      <line rowAcum="12" row="last" col="2" row2="last" col2="200" />
   </package>      
   <!-- 17 Total articulos -->
   <package>
      <field textLine="Total Articulos:" rowAcum="15" textCol="13" fontName="Dialog" fontSize="12" fontStyle="0" row="last" col="200" width="13" type="NUMERIC" mask="#,###,###" />
   </package>  
   <!-- 18 Linea -->
   <package>
      <line row="last" rowAcum="20" col="2" row2="last" col2="200" />
   </package>  

     <!-- Estado factura -->
     <package>
       <field row="last" col="15" rowAcum="15" type="STRING" fontName="Dialog" fontSize="14" fontStyle="0" />
     </package>
   
     <package>
       <line row="last" rowAcum="15" col="2" row2="last" col2="200" />
     </package> 

  <!-- 19 Tercero -->
   <package validate="true">
      <field row="last" rowAcum="15"  col="15" type="STRING"/>
      <field row="last" rowAcum="15"  col="15" type="STRING"/>
      <field row="last" rowAcum="15"  col="15" type="STRING"/>
      <field row="last" rowAcum="15"  col="15" type="STRING"/>
      <field row="last" rowAcum="15"  col="15" type="STRING"/>
   </package>
   <!-- 20 Linea -->
   <package>
      <line row="last" rowAcum="15" col="2" row2="last" col2="200" />
   </package>  
   <!-- 21 Dias de credito -->
   <package validate="true">
      <field row="last" textLine="DIAS SEPARADO" fontName="Dialog" fontSize="10" fontStyle="0" rowAcum="15" textCol="13"  col="180" type="STRING"/>
   </package>

   <!-- 22 Vencimiento -->
   <package validate="true">
      <field row="last"  textLine="VENCIMIENTO" fontName="Dialog" fontSize="10" fontStyle="0" rowAcum="15" textCol="13"  col="140"  type="STRING"/>
   </package>
   <!-- 23 Linea -->
   <package>
      <line row="last" rowAcum="20" col="2" row2="last" col2="200" />
   </package>  
  <!-- 24 TRM -->
   <package>
      <field textLine="TRM ......" rowAcum="15" textCol="13" fontName="Dialog" fontSize="12" fontStyle="1" row="last" col="200" width="13" type="NUMERIC" mask="#,###,###.##" />
   </package>  
   <!-- 25 Total USD -->
   <package>
      <field textLine="Total US $ ......" rowAcum="15" textCol="13" fontName="Dialog" fontSize="12" fontStyle="1" row="last" col="200" width="13" type="NUMERIC" mask="#,###,###.##" />
   </package>  
   <!-- 26 Paquete blanco linea -->
   <package>
      <line row="last" rowAcum="20" col="2" row2="last" col2="200" />
   </package>        
   <!-- 27 Pie de pagina -->
   <package>
	<text fontName="Dialog" fontSize="7" fontStyle="1" rowAcum="10" row="last" col="8" type="STRING">No se aceptan cambios de mercancía en promoción.</text>
	<text fontName="Dialog" fontSize="7" fontStyle="1" rowAcum="10" row="last" col="8" type="STRING">Plazo máximo para cambio de mercancía 8 días.</text>
	<text fontName="Dialog" fontSize="7" fontStyle="1" rowAcum="10" row="last" col="8" type="STRING">Plazo máximo por reclamo frente a calidad en</text>
	<text fontName="Dialog" fontSize="7" fontStyle="1" rowAcum="10" row="last" col="8" type="STRING">calzado,prendas de vestir, por defectos de fabrica</text>
	<text fontName="Dialog" fontSize="7" fontStyle="1" rowAcum="10" row="last" col="8" type="STRING">mas no por maltrato 30 dias a partir de la compra</text>
	<text fontName="Dialog" fontSize="7" fontStyle="1" rowAcum="10" row="last" col="8" type="STRING">En ningún caso hay devolución de dinero.</text>
	<text fontName="Dialog" fontSize="7" fontStyle="1" rowAcum="10" row="last" col="8" type="STRING">No se aceptan cambios ni devoluciones en vestidos</text>
	<text fontName="Dialog" fontSize="7" fontStyle="1" rowAcum="10" row="last" col="8" type="STRING">de fiesta ni accesorios.</text>
	<text fontName="Dialog" fontSize="7" fontStyle="1" rowAcum="10" row="last" col="8" type="STRING">Plazo máximo para la mercancia separada es 30 días</text>
	<text fontName="Dialog" fontSize="7" fontStyle="1" rowAcum="10" row="last" col="8" type="STRING">La mercancía separada no tiene cambios.</text>
	<text fontName="Dialog" fontSize="7" fontStyle="1" rowAcum="10" row="last" col="8" type="STRING">No se aceptan retiros parciales en separados.</text>
	<text fontName="Dialog" fontSize="7" fontStyle="1" rowAcum="10" row="last" col="8" type="STRING">Para cualquier reclamo es necesario</text>
	<text fontName="Dialog" fontSize="7" fontStyle="1" rowAcum="15" row="last" col="8" type="STRING">presentar este documento.</text>
	<text rowAcum="10" row="last" col="15" type="STRING" importValue="true">direccion</text>
	<text rowAcum="10" row="last" col="15" type="STRING" importValue="true">telefono</text>
	<text rowAcum="10" row="last" col="15" type="STRING" importValue="true">email</text>
	<text rowAcum="20" row="last" col="15" type="STRING" importValue="true">ciudad</text>
	<text fontName="Dialog" fontSize="14" fontStyle="1" rowAcum="30" row="last" col="12" type="STRING">GRACIAS POR SU COMPRA</text>
	<text fontName="Dialog" fontSize="6" fontStyle="1" rowAcum="8" row="last" col="12" type="STRING">Factura Elaborada Con E-Maku</text>
	<text fontName="Dialog" fontSize="6" fontStyle="1" rowAcum="30" row="last" col="12" type="STRING">Licenciado Bajo GPL V 2.0</text>
   </package>

</emaku_template>
