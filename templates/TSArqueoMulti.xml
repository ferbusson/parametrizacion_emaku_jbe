   </package>     
  17 <!-- Linea -->
   <package>
      <line row="last" rowAcum="20" col="2" row2="last" col2="200" />
   </package>     
  18<!--Titulo informacion de recaudo-->
   <package>
      <text fontName="Dialog" fontSize="12" fontStyle="1" rowAcum="20" row="last" col="8" type="STRING">Informacion de Recaudo</text>
   </package>   

   19 <!-- Tabla recaudo-->
   <package>
      <subpackage rowInit="last" rowAcum="8">
         <field fontName="Dialog" fontSize="10" fontStyle="0" col="15" type="STRING" incrementRow="false"/>
         <field fontName="Dialog" fontSize="10" fontStyle="0" col="200" incrementRow="true" width="13" type="NUMERIC" mask="#,###,###.##" />
      </subpackage>
   </package>


  20 <!-- Total recaudo -->
   <package>
      <field textLine="Total Recaudo......" rowAcum="20" textCol="12" fontName="Dialog" fontSize="10" fontStyle="0" row="last" col="200" width="13" type="NUMERIC" mask="#,###,###.##" />
   </package>  


  21 <!-- Linea -->
   <package>
      <line row="last" rowAcum="20" col="2" row2="last" col2="200" />
   </package>  
  22 <!-- Total movimiento del dia -->
   <package>
      <field textLine="Total Mov. del Dia......" rowAcum="15" textCol="12" fontName="Dialog" fontSize="10" fontStyle="1" row="last" col="200" width="13" type="NUMERIC" mask="#,###,###.##" />
   </package>  

  23 <!-- Efectivo recaudado -->
