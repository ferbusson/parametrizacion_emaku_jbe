   </package>  


   12 <!-- Total ingresos del turno-->
   <package>
      <field textLine="Total Ingresos......" rowAcum="20" textCol="12" fontName="Dialog" fontSize="10" fontStyle="1" row="last" col="200" width="13" type="NUMERIC" mask="#,###,###.##" />
   </package>  

  13 <!-- Linea -->
   <package>
      <line row="last" rowAcum="20" col="2" row2="last" col2="200" />
   </package>     
  14<!--Titulo Egresos del turno-->
   <package>
      <text fontName="Dialog" fontSize="12" fontStyle="1" rowAcum="20" row="last" col="8" type="STRING">Egresos del Turno</text>
