   </package>  

  26 <!-- Linea -->
   <package>
      <line row="last" rowAcum="20" col="2" row2="last" col2="200" />
   </package>  
  27<!--Titulo Observaciones-->
   <package>
      <text fontName="Dialog" fontSize="12" fontStyle="1" rowAcum="20" row="last" col="8" type="STRING">Observaciones:</text>
   </package>

  28 <!-- Observaciones -->
   <package>
      <field fontName="Dialog" fontSize="8" fontStyle="0" row="last" col="1"  width="50"  height="5" rowAcum="10" type="TEXT"/>
   </package> 

   29 <!-- Linea -->
   <package>
      <line rowAcum="40" row="last" col="2" row2="last" col2="200" />
   </package>     

   30 <!-- Linea firma-->
   <package>
      <line rowAcum="15" row="last" col="32" row2="last" col2="170" />
   </package>     
  31<!--Titulo Elaboro-->
   <package>
      <text fontName="Dialog" fontSize="12" fontStyle="1" rowAcum="30" row="last" col="75" type="STRING">Elabor√≥</text>
   </package>

   
   32 <!-- Pie de pagina -->
   <package>

      <text fontName="Dialog" fontSize="8" fontStyle="1" rowAcum="10" row="last" col="15" type="STRING" importValue="true">direccion</text>

      <text fontName="Dialog" fontSize="8" fontStyle="1" rowAcum="10" row="last" col="15" type="STRING" importValue="true">telefono</text>

     <text rowAcum="10" row="last" col="15" type="STRING">fabflyestiloamericano@gmail.com</text>

      <text fontName="Dialog" fontSize="8" fontStyle="1" rowAcum="20" row="last" col="15" type="STRING" importValue="true">ciudad</text>


      
      <text fontName="Dialog" fontSize="6" fontStyle="1" rowAcum="8" row="last" col="12" type="STRING">Comprobante Elaborado Con E-Maku</text>

