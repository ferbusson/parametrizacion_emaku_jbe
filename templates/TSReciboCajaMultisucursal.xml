	</package>
	6 VALOR
	<package>
		<field row="183" col="170" width="10" type="NUMERIC" mask="#,###,###" fontSize="14" fontName="Sans"/>
	</package>
	7 NOMBRE
	<package>
		<field row="202" col="5" width="10" type="STRING" fontSize="10" fontName="Sans"/>
	</package>
	8 DOCUMENTO
	<package>
		<field row="215" col="5" width="10" type="STRING" fontSize="10" fontName="Sans"/>
	</package>

	9 CONCEPTO
